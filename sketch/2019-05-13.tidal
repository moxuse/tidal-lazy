d1 
$ unison 
  "wnoise" ("[mbd2:6, mst:9 mhh:6]*4" 
  |+ mGain 
  |+| cps ("0.5 0.75 0.6")
  |+ n (slow 3 $ "1 8 ~ 1 9"))
$ s "[wnoise, msn:4]*4" 
|+ sustain (slow 8 (range 1 0.2 $ sine))
|+ mGain
|+ slowPanR 3
|+ conv (slow 12 (range 0.2 0.9 sine))
|+ cutoff (slow 16 (range 8000 300 $ sine))
|+ up (scale "lydian" "[0, 4, 7, 15, 23]" + "-4")
|+ cps (slow 8 (range 0.4 5.8 $ sine))
|* cps (slow 5 "0.4 0.5 0.3 [0.5 0.7 0.93 0.6] 0.8")
|+ n (slow 4 "3 5 13")
|+ room 0.1

hush

